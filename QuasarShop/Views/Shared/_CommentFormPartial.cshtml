@model CommentViewModel

<form method="post">
    <label asp-for="Text"></label>
    <textarea asp-for="Text" class="form-control"></textarea>
    <div class="py-2 rating-wrapper">
        @for (int i = 0; i < 5; i++)
        {
            <i class="bi bi-star me-2" data-index="@i"></i>
        }
    </div>
</form>

